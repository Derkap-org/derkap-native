{
  "name": "derkap-native",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "prepare": "npx husky",
    "start": "npx expo start --tunnel",
    "start-local": "npx expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "db-pull": "supabase db pull && supabase db pull --schema auth,storage",
    "db-reset": "supabase db reset",
    "db-diff": "supabase db diff --use-migra -f",
    "db-seed": "supabase db dump --data-only -f supabase/seed.sql",
    "db-push": "supabase db push",
    "db-squash": "supabase migration squash",
    "db-types": "npx supabase gen types typescript localhost --schema public > types/supabase.ts"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@birdwingo/react-native-swipe-modal": "^1.2.3",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.0.0",
    "@supabase/supabase-js": "^2.46.1",
    "clsx": "^2.1.1",
    "cookie": ">=0.7.0",
    "expo": "^52.0.18",
    "expo-blur": "~14.0.1",
    "expo-camera": "~16.0.9",
    "expo-constants": "~17.0.3",
    "expo-device": "~7.0.1",
    "expo-font": "~13.0.1",
    "expo-linking": "~7.0.3",
    "expo-notifications": "~0.29.11",
    "expo-router": "~4.0.11",
    "expo-splash-screen": "~0.29.18",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.6",
    "expo-web-browser": "~14.0.1",
    "lucide-react-native": "^0.460.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-modalize": "^2.1.1",
    "react-native-reanimated": "~3.16.5",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-svg": "^15.8.0",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-web": "~0.19.10",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.2",
    "expo-clipboard": "~7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^18.0.7",
    "ajv": "^7.2.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "cookie": ">=0.7.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
