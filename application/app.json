{
  "expo": {
    "name": "Derkap",
    "slug": "derkap",
    "version": "2.4.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "derkap",
    "userInterfaceStyle": "automatic",
    "locales": {
      "fr": "./locales/fr.json"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8C4BF5"
    },
    "ios": {
      "buildNumber": "1",
      "supportsTablet": false,
      "bundleIdentifier": "com.nicoalz.derkap",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleLocalizations": ["fr"],
        "CFBundleDevelopmentRegion": "fr"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#8C4BF5"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Pour prendre tes meilleures photos !",
          "microphonePermission": "Pour entendre ta belle voix !",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/grotesque/grotesque.ttf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app can access your photos to let you share them with your friends."
        }
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1046f88e-e18e-446a-b82d-918ef49e273f"
      }
    },
    "owner": "nicoalz",
    "newArchEnabled": true
  }
}
